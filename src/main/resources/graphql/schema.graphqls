type Query {
    getAllbooks : [Book]
    bookById(id:Int):Book 
}

type Book {
    id: ID,
    name: String,
    pageCount: Int
    author: Author
}

type Author {
    id:ID,
    name: String
}

type Mutation {
    createCustomer(name: String!, email: String): Customer
}

type Customer {
    id: ID!
    name: String!
    email: String
}
